{
  "name": "sync",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "@primer/components": "^13.3.1",
    "@primer/octicons-react": "^9.1.1",
    "axios": "^0.21.1",
    "github-api": "^3.2.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "npm run lint && react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "http-proxy-middleware": "^0.19.1"
  }
}
